{
    "compilerOptions": {
        "moduleResolution": "node",
        "target": "es5",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "outDir": "./build",
        "allowSyntheticDefaultImports": true,
        "noImplicitUseStrict": true
    },
    "include": ["src/**/*"]
}
